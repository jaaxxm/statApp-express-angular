extends layout

block body

  header.app-header
      .navbar.navbar-inverse
        .navbar-inner
          .container-fluid
            button.btn.btn-navbar(type='button', data-toggle='collapse', data-target='.nav-collapse')
              span.icon-bar
              span.icon-bar
              span.icon-bar
            a.brand(href='#') Call Stats
            .nav-collapse.collapse
              p.navbar-text.pull-left
                | Logged in as
                a.navbar-link(href='#') Username
              ul.nav.pull-right
                li
                  a(href='#') Настройки
                li
                  a(href='') Выход
  nav.app-nav
    ul.nav.nav-list
      li
        a(href='#')
          i.icon-chevron-right
          | Главная
      li
        a(href='#contact')
          i.icon-chevron-right
          | Контакты
      li
        a(href='#')
          i.icon-chevron-right
          | Журнал звонков
      li
        a(href='#')
          i.icon-chevron-right
          | Звонки по дням
      li
        a(href='#')
          i.icon-chevron-right
          | ROI - возврат инвестиций




  div(ng-controller="AppCtrl")
    h2 Hello {{name}}
    ul.menu
      li
        a(href="view1") view1
      li
        a(href="view2") view2

    div(class="app-container", ng-view)

    div.text-center
      | Angular Socket.io seed app: v
      span(app-version)




  script(src='bower_components/jquery/jquery.min.js')
  script(src="/socket.io/socket.io.js")  
  script(src='bower_components/angular-unstable/angular.js')
  script(src="bower_components/angular-socket-io/socket.js")
  script(src='bower_components/angular-ui/build/angular-ui.js')
  script(src='bower_components/angular-bootstrap/ui-bootstrap-tpls.js')
  script(src='bower_components/d3/d3.min.js')
  script(src='bower_components/d3-angular/dist/angular-d3.min.js')
  script(src='bower_components/ng-grid/ng-grid-2.0.7.min.js')
  
  script(src="js/app.js")
  script(src="js/services.js")
  script(src="js/controllers.js")
  script(src="js/filters.js")
  script(src="js/directives.js")

  script(src='bower_components/angular-resource/angular-resource.js')
  script(src='bower_components/angular-cookies/angular-cookies.js')
  script(src='bower_components/angular-sanitize/angular-sanitize.js')  
